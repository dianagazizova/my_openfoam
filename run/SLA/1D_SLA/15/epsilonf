FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       surfaceVectorField;
    location    "15";
    object      epsilonf;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

oriented        oriented;

internalField   nonuniform List<vector> 3((-0 -0 4) (-0 -0 2) (-0 -0 -0));

boundaryField
{
    bottom
    {
        type            calculated;
        value           uniform (-0 -0 -4);
    }
    top
    {
        type            calculated;
        value           uniform (-0 -0 -0);
    }
    frontAndBack
    {
        type            empty;
        value           nonuniform List<vector> 0();
    }
    left
    {
        type            empty;
        value           nonuniform List<vector> 0();
    }
    right
    {
        type            empty;
        value           nonuniform List<vector> 0();
    }
}


