FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "15";
    object      nLayer;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   nonuniform List<scalar> 4(1 1 2 2);

boundaryField
{
    bottom
    {
        type            calculated;
        value           uniform 1;
    }
    top
    {
        type            calculated;
        value           uniform 2;
    }
    frontAndBack
    {
        type            empty;
    }
    left
    {
        type            empty;
    }
    right
    {
        type            empty;
    }
}


